{
	"name": "vscodesw",
	"displayName": "VSCode Magik Smallworld",
	"publisher": "alexhenriquepv",
	"author": {
		"name": "Alex Paiva"
	},
	"description": "Extension for compile magik code and language support",
	"repository": {
		"type": "git",
		"url": "https://github.com/alexhenriquepv/vscode-sw"
	},
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": ["Programming Languages"],
	"keywords": ["magik","ge","energisa","smallworld","emacs"],
	"icon": "src/wizard.png",
	"activationEvents": [
		"onCommand:vscodesw.sessao",
		"onCommand:vscodesw.compila"
	],
	"main": "src/extension.js",
	"contributes": {
		"configuration": {
			"title": "VSCodesw",
			"properties": {
				"vscodesw.productPath": {
					"type": ["string", "null"],
					"default": "C:/GE_Smallworld/Core430/product",
					"description": "Specifies the path of gis.exe."
				}
			}
		},
		"commands": [
			{
				"command": "vscodesw.sessao",
				"title": "MGK open session"
			},
			{
				"command": "vscodesw.compila",
				"title": "MGK compilare current code"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "vscodesw.sessao",
					"group": "navigation",
					"when": "editorLangId == magik"
				},
				{
					"command": "vscodesw.compila",
					"group": "navigation",
					"when": "editorLangId == magik"
				}
			]
		},
		"languages": [
			{
				"id": "magik",
				"aliases": [
					"magik",
					"Smallworld Magik",
					"Energisa"
				],
				"extensions": [
					".magik"
				],
				"filenames": [],
				"configuration": "./cfg/magik-configuration.json"
			},
			{
				"id": "swgis",
				"aliases": [
					"swgis",
					"Smallworld GIS"
				],
				"extensions": [
					".msg"
				],
				"filenames": [
					"gis_aliases",
					"product.def",
					"module.def",
					"load_list.txt",
					"patch_list.txt"
				],
				"configuration": "./cfg/swgis-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "magik",
				"scopeName": "source.magik",
				"path": "./cfg/magik.grammars.json"
			},
			{
				"language": "swgis",
				"scopeName": "source.swgis",
				"path": "./cfg/swgis.grammars.json"
			}
		],
		"snippets": [
			{
				"language": "magik",
				"path": "./cfg/magik.snippets.json"
			},
			{
				"language": "swgis",
				"path": "./cfg/swgis.snippets.json"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"node-process-windows": "0.0.2"
	}
}
